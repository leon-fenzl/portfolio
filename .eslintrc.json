{
"extends":[
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-ally/recommended",
    "prettier"
],

"plugins":["react", "jsx-ally", "import"],

"rules":{
    "react/prop-types":0,
    "react/react-in-jsx-scope":0
},
"parserOptions":{
    "ecmaVersion":2025,
    "sourceType":"module",
    "ecmaFeatures":{"jsx":true}
},
"env":{
    "es6":true,
    "browser":true,
    "node":true
},
"settings":{
    "react":{
        "version":"detect"
    }
}
}